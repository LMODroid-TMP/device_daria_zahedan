# Battery
genfscon sysfs /devices/platform/11d01000.i2c7/i2c-7/7-0064/power_supply       u:object_r:sysfs_batteryinfo:s0

# Charger
genfscon sysfs /devices/platform/charger/cmd_charge_disable                    u:object_r:sysfs_charge_disable:s0
genfscon sysfs /devices/platform/common_node/charger_input_limit               u:object_r:sysfs_charge_current:s0

# Wakeup
genfscon sysfs /devices/platform/11d01000.i2c7/i2c-7/7-0066/wakeup             u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11d01000.i2c7/i2c-7/7-0064/power_supply/cw-bat/wakeup143 u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/11cb0000.i2c3/i2c-3/3-0066/wakeup/wakeup141   u:object_r:sysfs_wakeup:s0
genfscon sysfs /devices/platform/pca_dv2_algo/wakeup                           u:object_r:sysfs_wakeup:s0

# Vibrator
genfscon sysfs /devices/platform/11f00000.i2c6/i2c-6/6-005a/leds/vibrator      u:object_r:sysfs_vibrator:s0

# Touch gesture
genfscon sysfs /devices/platform/goodix_ts.0/gesture                           u:object_r:sysfs_touchgesture:s0

# Light
genfscon sysfs /devices/platform/common_node/hbmstate                          u:object_r:sysfs_leds:s0
genfscon sysfs /devices/platform/11d00000.i2c5/i2c-5/5-0034/mt6360_pmu_fled.3.auto/mt6360_pmu_fled.0/rt-flash-led.0/flashlight/mt-flash-led1/torch_brightness      u:object_r:sysfs_torch:s0
genfscon sysfs /devices/platform/11d00000.i2c5/i2c-5/5-0034/mt6360_pmu_fled.3.auto/mt6360_pmu_fled.0/rt-flash-led.0/flashlight/mt-flash-led1/torch_max_brightness  u:object_r:sysfs_torch:s0
genfscon sysfs /devices/platform/11d00000.i2c5/i2c-5/5-0034/mt6360_pmu_fled.3.auto/mt6360_pmu_fled.1/rt-flash-led.1/flashlight/mt-flash-led2/torch_brightness      u:object_r:sysfs_torch:s0
genfscon sysfs /devices/platform/11d00000.i2c5/i2c-5/5-0034/mt6360_pmu_fled.3.auto/mt6360_pmu_fled.1/rt-flash-led.1/flashlight/mt-flash-led2/torch_max_brightness  u:object_r:sysfs_torch:s0
